{
    "compilerOptions": {
        "noImplicitAny": true,
        "noUnusedParameters": false,
        "noUnusedLocals": true,
        "skipLibCheck": true,
        "moduleResolution": "node",
        "importHelpers": true,
        "removeComments": true,
        "sourceMap": false,
        "declaration": true,
        "declarationMap": false,
        "allowSyntheticDefaultImports": true,
        "outDir": "./dist"
    }
}
